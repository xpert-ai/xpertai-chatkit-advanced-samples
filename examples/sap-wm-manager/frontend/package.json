{
  "name": "zb001",
  "version": "0.0.1",
  "description": "An SAP Fiori application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "dependencies": {},
  "devDependencies": {
    "@ui5/cli": "^4.0.33",
    "@sap/ux-ui5-tooling": "1",
    "rimraf": "^5.0.5",
    "@ui5/linter": "^1.20.7",
		"@ui5/middleware-code-coverage": "^2.0.2",
		"eslint": "^9.39.2",
		"globals": "^17.0.0",
		"local-web-server": "^5.4.0",
		"ui5-test-runner": "^5.13.0",
    "@eslint/js": "^9.39.2"
  },
	"engines": {
		"node": "^20.11.0 || >=22.0.0",
		"npm": ">= 8"
	},
  "scripts": {
    "start": "fiori run --open \"/test/flpSandbox.html?sap-ui-xx-viewCache=false#aicomzb001-display\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"/test/flpSandbox.html?sap-ui-xx-viewCache=false#aicomzb001-display\"",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open \"/index.html?sap-ui-xx-viewCache=false\"",
    "int-test": "fiori run --open \"test/integration/opaTests.qunit.html\"",
    "start-variants-management": "fiori run --open \"/preview.html?sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#aicomzb001-display\"",
    "unit-test": "fiori run --open \"test/unit/unitTests.qunit.html\"",
    "undeploy": "npm run build && fiori undeploy --config ui5-deploy.yaml",
    "deploy-test": "npm run build && fiori deploy --config ui5-deploy.yaml --testMode true"
  },
  "sapuxLayer": "CUSTOMER_BASE"
}
